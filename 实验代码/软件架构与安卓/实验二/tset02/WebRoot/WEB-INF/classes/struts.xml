<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="house" namespace="/house" extends="struts-default">
	    <action name="addHouse" class="com.action.AddHouse">
	      <result name="success">../success.html</result>
	      <result name="error">../error.html</result>
	    </action>
	   <action name="findHouse" class="com.action.FindHouse">
	   	  <result name="success">findHouse.jsp</result>
	   	  <result name="error">../error.html</result>
	   </action>
	   <action name="updateHouse" class="com.action.UpdateHouse">
	   		<result name="success">../success.html</result>
	   		<result name="error">../error.html</result>
	   </action>
	   <action name="deleteHouse" class="com.action.DeleteHouse">
	   		<result name="success">../success.html</result>
	   		<result name="error">../error.html</result>
	   </action>
  </package>
</struts>    
